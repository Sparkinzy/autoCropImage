RewriteEngine On

# autoCropImage 程序 URI 模式匹配规则
RewriteRule ^.*(?:gif|jpg|png)(?:_)([0-9]+)x([0-9]+)(?:m([1-5]))?(?:v([^.]*))?(?:.)?(?:gif|jpg|png)?$ autoCropImage/autoCropImage.php [L,QSA]
# autoCropImage 程序 QUERY 模式匹配规则
RewriteCond %{QUERY_STRING} ^([0-9]+)x([0-9]+)(?:m([1-5]))?(?:v([0-9]*))?$
RewriteRule ^.*\.(gif|jpg|png)$ autoCropImage/autoCropImage.php [L,QSA]
